//PK10数据接口

//API http://游戏服务器域名/api/live?code=pk10&t=请求时间戳
//code: 游戏类型:pk10表示游戏
//t   : 时间戳，防缓存数据

//接口返回数据格式

{
    "time":服务器当前时间戳(秒),
    "current":{                 //当前期数据
        "periodNumber":"期号NO",
        "period":"期号NO",
        "periodDate":"期号NO",
        "awardTime":"开奖时间",
        "awardNumbers":"开奖号码排序"
    },
    "next":{                    //下一期数据
        "periodNumber":"期号NO",
        "period":"期号NO",
        "periodDate":"期号NO",
        "awardTime":"开奖时间",
        "awardTimeInterval":距离开奖剩余时间（秒）,
        "delayTimeInterval":请求频率（固定填写10）
    }
}







